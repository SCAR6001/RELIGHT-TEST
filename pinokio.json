{
  "name": "AI Image Relighter",
  "description": "Relight any image using AI (SwitchLight-inspired)",
  "version": "1.0.0",
  "author": "You",
  "type": "python",
  "python": "3.10",

  "scripts": {
    "install": {
      "cmd": "pip install -r app/requirements.txt"
    },
    "run": {
      "cmd": "uvicorn app.main:app --host 0.0.0.0 --port 7860",
      "daemon": true,
      "open": true
    }
  },

  "ports": [
    {
      "port": 7860,
      "protocol": "http"
    }
  ]
}
