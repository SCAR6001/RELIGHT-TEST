{
  "name": "AI Image Relighter",
  "description": "Relight any image using AI (SwitchLight-inspired)",
  "version": "1.0",
  "type": "app",
  "entry": "app/main.py",
  "python": "3.10",
  "install": [
    "pip install -r app/requirements.txt"
  ],
  "run": [
    "uvicorn app.main:app --host 0.0.0.0 --port 7860"
  ],
  "ports": [
    {
      "containerPort": 7860,
      "hostPort": 7860
    }
  ]
}
